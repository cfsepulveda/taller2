<?xml version="1.0" encoding="UTF-8"?>
<cloud_computing:Proveedor xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:cloud_computing="https://uniandes.edu.co/cloud_computing">
  <conexion nombreConexion="Login">
    <atributos nombre="user" tipoDato="text" tipoConexion="Basica"/>
    <atributos nombre="password" tipoDato="password" tipoConexion="basica"/>
  </conexion>
  <ambientedesplegue>
    <servidor xsi:type="cloud_computing:ServidorAplicacion" nombre="TOMCAT" servidor="//@ambientedesplegue.0/@servidor.2"/>
    <servidor xsi:type="cloud_computing:ServidorBaseDeDatos" nombre="PostgresSQL"/>
    <servidor xsi:type="cloud_computing:ServidorAlmacenamiento" nombre="Repositorios" tamanoInicial="50000GB"/>
    <servidor xsi:type="cloud_computing:ServidorAplicacion" nombre="GLASSFISH" servidor="//@ambientedesplegue.0/@servidor.2"/>
  </ambientedesplegue>
  <vpc name="internet">
    <vpc name="SIstemas">
      <rules type="Custom TCP" protocol="MAL" portRange="1024 to 65537" Source="localhost" description="Custom TCP 1024 to 65535"/>
      <rules type="Custom UDP" protocol="UDP" portRange="1024 to 65535" Source="localhost" description="Custom UDP 1024 to 65535"/>
      <rules type="DNS UDP" protocol="UDP" portRange="5000" Source="lcoalhost" description="DNS UDP 53"/>
      <rules type="NFS" protocol="TCP" portRange="2141" Source="localhost"/>
      <subred name="dev" cidr="192.168.0.1/24"/>
      <securitygroup name="Grupo seguridad 1" description="Grupo seguridad 1"/>
    </vpc>
  </vpc>
</cloud_computing:Proveedor>
